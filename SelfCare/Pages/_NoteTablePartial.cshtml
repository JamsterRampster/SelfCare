@using SelfCare
@namespace SelfCare.Pages
@model List<SelfCare.Models.Note>

<table>
    <thead>
        <tr>
            <th>Note Id</th>
            <th>Patient Id</th>
            <th>Title</th>
            <th>Body</th>
            <th>Date Updated</th>
            <th>Date Created</th>
        </tr>
    </thead>
    <tbody>
         @foreach (var note in Model)
        {
            <tr>
                <td>@note.NoteId</td>
                <td>@note.PatientId</td>
                <td>@note.Title</td>
                <td>@note.Body</td>
                <td>@note.DateUpdated</td>
                <td>@note.DateCreated</td>
            </tr>
        }
    </tbody>
</table>